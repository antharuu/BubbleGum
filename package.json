{
  "name": "bubblegum-css",
  "version": "0.8.0",
  "description": "Simple and efficiency Css framework",
  "main": "main.js",
  "keywords": [
    "css",
    "bubblegum",
    "bubble",
    "gum",
    "attributes",
    "bootstrap",
    "framework",
    "scss",
    "sass"
  ],
  "scripts": {
    "WATCH SASS": "sass --watch css/bubblegum.scss:css/bubblegum.css",
    "COMPILE SASS": "sass css/bubblegum.scss:css/bubblegum.min.css",
    "COMPILE SASS MIN": "sass --style=compressed css/bubblegum.scss:css/bubblegum.min.css",
    "WATCH SASS 1.0": "sass --watch css_1.0/bubblegum.scss:css_1.0/bubblegum.css",
    "COMPILE SASS 1.0": "sass css_1.0/bubblegum.scss:css_1.0/bubblegum.min.css",
    "COMPILE SASS MIN 1.0": "sass --style=compressed css_1.0/bubblegum.scss:css_1.0/bubblegum.min.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antharuu/BubbleGum.git"
  },
  "author": "Antharuu <antho.eclips@gmail.com> (https://github.com/antharuu/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antharuu/BubbleGum/issues"
  },
  "homepage": "https://github.com/antharuu/BubbleGum#readme",
  "dependencies": {
    "nib": "^1.1.2",
    "prismjs": "^1.23.0",
    "stylus": "^0.54.8"
  }
}
